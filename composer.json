{
    "name": "richardkeep/validate",
    "description": "A simple Laravel 5 package to validate form inputs on the go",
    "type": "beta",
    "authors": [
        {
            "name": "Richard Keep"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/validation": "^5.8|^6.0",
        "illuminate/http": "^5.8|^6.0",
        "illuminate/support": "^5.8|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Richardkeep\\Validate\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                 "Richardkeep\\Validate\\RichardkeepServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
